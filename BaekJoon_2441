#include <iostream>
#include <string>

using namespace std;

int main() {
	int N = 0;
	cin >> N;

	string star = "*";
	string blank = " ";
	for (int i = N; i >= 1; i--) {
		for (int j = 0; j < (N - i); j++) {
			cout << blank;

		}
		for (int j = 0; j < i; j++) {
			cout << star;
			
		}
		cout << endl;
	}
	return 0;
}
